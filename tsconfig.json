{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@content/*": ["src/content/*"],
      "@icons/*": ["src/components/icons/*"],
      "@root/*": ["./*"],
      "@src/*": ["src/*"],
      "@typedefs/*": ["src/types/*"]
    }
  }
}
